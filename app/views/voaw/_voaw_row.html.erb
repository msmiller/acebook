<tr>
  <td nowrap>
    <a href="/show/<%= voaw_row.user_id %>" data-reveal-id="myModal" data-reveal-ajax="true">
      <%= image_pack_tag "media/images/icons/#{voaw_row.get_icon}.gif", :class => 'miniicon' %><b><%= voaw_row.handle %></b></a>
      <br>
      <span class="smallertext"><%= voaw_row.squadron %></span>
    </td>
  <% if @current_user -%>
  <td nowrap>
    <%= render :partial => "social_icons", :locals => { :socials => voaw_row.user.social_info } %>
    <%= voaw_row.name %>
    <br>
    <span class="smallertext"><%= truncate(voaw_row.row_addr, length: 30)  %></span>
  </td>
  <% end -%>
  <td><%= voaw_row.aw_cpid %></td>
  <td><%= voaw_row.wb_cpid %></td>
  <td><%= voaw_row.ah_cpid %></td>
  <td><%= voaw_row.year_started %></td>
</tr>